{
  "name": "payment-analyzer-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:trace": "NEXT_TURBOPACK_TRACING=1 next dev --turbo",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build -c next.config.analyze.js",
    "build:prod": "cross-env NODE_ENV=production next build -c next.config.production.ts",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "next build --debug && echo \"Cleaning completed\"",
    "clean:win": "if exist .next rd /s /q .next && if exist out rd /s /q out && if exist node_modules\\.cache rd /s /q node_modules\\.cache",
    "reset": "pnpm clean:win && pnpm install",
    "analyze": "cross-env ANALYZE=true pnpm build:analyze",
    "trace:analyze": "next internal trace .next/trace-turbopack",
    "db:migrate": "node scripts/run-migrations.js",
    "db:maintenance": "node scripts/run-maintenance.js"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.85.0",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "idb": "^8.0.0",
    "lucide-react": "^0.542.0",
    "lz-string": "^1.5.0",
    "next": "15.5.0",
    "pdfjs-dist": "^5.4.54",
    "react": "19.1.0",
    "react-calendar": "^6.0.0",
    "react-day-picker": "^9.6.8",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.0",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.0"
  }
}